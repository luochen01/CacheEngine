  	<type>Web</type> 
	<!-- Mandatory field -->
  	<description>[description]</description>  
  	<lifecycleState>Deployed</lifecycleState> 
  	<issueURL>Enter your jira project url or equivalent</issueURL> 
	<!-- Optional field: Default value will be used if not specified -->
